            android:paddingTop="@dimen/rightmargin">

            <EditText
                android:id="@+id/editText18"
                android:layout_width="@dimen/edittextwidth"
                android:layout_height="wrap_content"
                android:background="@null"
                android:cursorVisible="false"
                android:inputType="textNoSuggestions"
                android:textColor="@color/textcolor" />

            <Switch
                android:id="@+id/switch18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/switchmarginleft"
                android:layout_marginStart="@dimen/switchmarginleft" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll8"
            android:paddingTop="@dimen/rightmargin">

            <EditText
                android:id="@+id/editText19"
                android:layout_width="@dimen/edittextwidth"
                android:layout_height="wrap_content"
                android:background="@null"
                android:cursorVisible="false"
                android:inputType="textNoSuggestions"
                android:textColor="@color/textcolor" />

            <Switch
                android:id="@+id/switch19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/switchmarginleft"
                android:layout_marginStart="@dimen/switchmarginleft" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll9"
            android:paddingTop="@dimen/rightmargin">

            <EditText
                android:id="@+id/editText20"
                android:layout_width="@dimen/edittextwidth"
                android:layout_height="wrap_content"
